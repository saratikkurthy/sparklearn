{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SaratDF"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"MySql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySql1'"
		},
		"linkedService1_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'linkedService1'"
		},
		"linkedService1_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'linkedService1'"
		},
		"linkedService1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'linkedService1'"
		},
		"linkedService1_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "91d9fe4f-ee4b-4dec-af3b-74d7c321f59a"
		},
		"linkedService1_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "SaratLearningGroup"
		},
		"linkedService1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "sparklearn"
		},
		"linkedService1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "791b5363-fa9d-42a1-8b34-90ff81862f56"
		},
		"Trigger_k75_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_k75_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SaratDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"MySql1_connectionString": {
						"value": "[parameters('MySql1_connectionString')]"
					},
					"linkedService1_clusterPassword": {
						"value": "[parameters('linkedService1_clusterPassword')]"
					},
					"linkedService1_clusterSshPassword": {
						"value": "[parameters('linkedService1_clusterSshPassword')]"
					},
					"linkedService1_servicePrincipalKey": {
						"value": "[parameters('linkedService1_servicePrincipalKey')]"
					},
					"linkedService1_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('linkedService1_properties_typeProperties_hostSubscriptionId')]"
					},
					"linkedService1_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('linkedService1_properties_typeProperties_clusterResourceGroup')]"
					},
					"linkedService1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('linkedService1_properties_typeProperties_servicePrincipalId')]"
					},
					"linkedService1_properties_typeProperties_tenant": {
						"value": "[parameters('linkedService1_properties_typeProperties_tenant')]"
					},
					"Trigger_k75_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_k75_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_k75_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_k75_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}